<h1>all task</h1>

<ul>
<% @tasks.each do |task| %>
 <li><%= link_to task.title, task_path(task) %></li>

  <% if task.completed %>
    <i class="far fa-check-square"></i>
      <% else %>
      <i class="far fa-square"></i>
  <% end %>


  <br>
  <br>

<% end %>
</ul>
  <%= link_to 'add new task', tasks_new_path%>
